{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://web4.io/schemas/Web4BirthCertificate.json",
  "title": "Web4BirthCertificate",
  "type": "object",
  "required": [
    "@context",
    "type",
    "entity",
    "citizenRole",
    "society",
    "lawOracle",
    "lawVersion",
    "birthTimestamp",
    "witnesses",
    "genesisBlock"
  ],
  "properties": {
    "@context": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "type": {
      "const": "Web4BirthCertificate"
    },
    "entity": {
      "type": "string"
    },
    "citizenRole": {
      "type": "string"
    },
    "society": {
      "type": "string"
    },
    "lawOracle": {
      "type": "string"
    },
    "lawVersion": {
      "type": "string"
    },
    "birthTimestamp": {
      "type": "string",
      "format": "date-time"
    },
    "witnesses": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "genesisBlock": {
      "type": "string"
    },
    "initialRights": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "initialResponsibilities": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}